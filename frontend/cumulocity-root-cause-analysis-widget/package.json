{
  "name": "cumulocity-root-cause-analysis-widget",
  "version": "2.0.1",
  "description": "This is an Angular widget, which is designed to display the AD chart and displays the RCA bar chart whenever AD > 1.",
  "author": "Sabreen Irfana",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildPatch": "cd projects/gp-rca && npm version patch && ng build gp-rca && cd ../../dist/gp-rca && npm pack && move *.tgz ../",
    "buildMinor": "cd projects/gp-rca && npm version minor && ng build gp-rca && cd ../../dist/gp-rca && npm pack && move *.tgz ../",
    "buildMajor": "cd projects/gp-rca && npm version major && ng build gp-rca && cd ../../dist/gp-rca && npm pack && move *.tgz ../",
    "serve": "ng build gp-rca && npm i dist/gp-rca && ng s",
    "runtime": "gulp --gulpfile ./runtime/gulpfile.js",
    "prettier:all": "cd projects/gp-rca/src && prettier --write \"**/*.@(js|ts|html|md|css|less|json|yaml)\" \"!**/*.min.*\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@angular/upgrade": "11.1.2",
    "@c8y/client": "^1011.0.12",
    "@c8y/ng1-modules": "^1011.0.12",
    "@c8y/ngx-components": "1011.0.12",
    "@c8y/style": "^1011.0.12",
    "@ng-select/ng-select": "^6.1.0",
    "chart.js": "^2.9.3",
    "gp-rca": "file:dist/gp-rca",
    "ng2-charts": "^2.4.3",
    "ngx-bootstrap": "6.2.0",
    "prettier": "^2.6.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular-devkit/build-ng-packagr": "~0.803.27",
    "@angular/cdk": "~11.2.13",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "core-js": "^2.5.4",
    "css-loader": "^3.5.3",
    "del": "^5.1.0",
    "delay": "^4.3.0",
    "fs-extra": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-inject-string": "^1.1.2",
    "gulp-json-modify": "^1.0.2",
    "gulp-replace": "^1.0.0",
    "gulp-zip": "^5.0.1",
    "i": "^0.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
